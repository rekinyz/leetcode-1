'use strict';

const assert = require('assert');
const _ = require('lodash');

const { template } = require('./');

describe('#template', () => {

  const tests = [$1];

  _.forEach(tests, ({ $2 result }) => {
    it(`$3 -> ${result}`, () => {
      assert.deepStrictEqual(template($4), result);
    });
  });
});
